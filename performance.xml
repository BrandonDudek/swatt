<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="OAC AT Performance Test Suite" verbose="1" group-by-instances="true" parallel="instances" thread-count="100">

    <!-- This TestNG Test Suite will run the Performance/Load Tests For this Project. -->

    <listeners>
        <listener class-name="xyz.swatt.testng.reporters.TestNgMatrixReporter"/>
    </listeners>

    <test name="OAC AT Performance Test">
        <classes>
            <class name="xyz.swatt.tests.selenium.temp.LogInPerfTests"/>
        </classes>
    </test>
</suite>
